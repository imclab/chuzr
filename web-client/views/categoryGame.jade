extends layout

block css
  link(href='/stylesheets/categoryGame.css', rel='stylesheet')
  link(href='http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css', rel='stylesheet')

block pagetitle
  div(class='page-title')
    img(src="/images/chuzr-labels/png/categories.png")

block content
  div(class = "row")
    div(id="productSpace" class = "span3 offset1")
      h3 Products


    div(id = "categoryContainer" class = "span8")
      h3 Categories
      table(id = "categoryTable" class = "table table-striped table-bordered")

  script(src='/js/categoryGame.js')
  script(src='http://code.jquery.com/ui/1.10.3/jquery-ui.js')

