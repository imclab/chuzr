extends layout

block css
  link(href='/stylesheets/play.css', rel='stylesheet')



block pagetitle
  div(class='page-title')
    img(src="/images/chuzr-labels/png/play.png")


block content
  div(class='play' id='play-game')
    div(id='game1' class='game')
      a(href='/chuzoff')
        p The Chuzoff
    div(id='game2' class='game')
      a(href='/rateme')
        p Rate Me
    div(id='game3' class='game')
      a(href='/categoryGame')
        p Category Game