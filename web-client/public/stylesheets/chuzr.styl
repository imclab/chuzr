html, body
  font: 16px "Lucida Grande", Helvetica, Arial, sans-serif  
  height: 100%
  width: 100%
  overflow: hidden

.navbar
  background-color: rgba(136, 159, 151, .5) !important
  margin-bottom: 0 !important

.table td
  border-top: none 

#wrap
  height: 100%
  width: 100%

a:hover
  opacity: 0.5

#content
  height:100%
  width:100%
  background-image:url('/images/texture3.png')


#main-content
  margin-top: 1%
  margin-bottom: 1%
  margin-left: 17%
  height: 84%
  background-color: rgba(256, 256, 256, .75)
  position: fixed
  width:82%
  overflow-y: auto !important

#side-nav
  top: 0px
  left: 0px
  padding-left: 10px
  padding-top: 50px
  padding-right: 10px
  background-color: rgba(256, 256, 256, .75)
  height: 100%
  width: 15%

#side-nav a img 
  display: block
  margin: auto

#side-nav-toggle
  margin-left: 0px
  padding-top: 20%
  padding-left: 5px
  opacity: 0.5
  background-color: rgba(256, 256, 256, .5)
  width: 7px
  height:100%

#user-profile
  padding: 10px
  background-color: rgba(19,164,157, .12)

.right-label
  text-align: right !important

.sub-nav
  padding-left: 5px
  background-color:rgba(19,164,157, .12)